{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::d01/*",
        "arn:aws:s3:::d02/*",
        "arn:aws:s3:::d01",
        "arn:aws:s3:::d02"
      ]
    }
  ]
}
